{
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true
  },
  "extends": "eslint:recommended",
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "rules": {
    "eol-last": ["error", "always"],
    "indent": ["error", 2],
    "no-console": "off",
    "no-undef": "off",
    "quotes": ["error", "single"],
    "semi": "error"
  }
}
